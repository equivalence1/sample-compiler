class Base
begin
    int x;
    fun init()
    begin
       return self 
    end

    fun foo(): int
    begin
        return 1
    end
end

class A extends Base
begin
    fun init()
    begin
        return self
    end

    fun foo(): int
    begin
        return 2
    end
end

fun foo_writer(Base x): int
begin
    write(x.foo());
    return 0
end

Base x;

x := new Base();
foo_writer(x);

x := new A();
foo_writer(x)
