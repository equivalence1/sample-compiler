class A
begin

    fun init()
    begin
        return self
    end

    fun foo(int x): int
    begin
        return x
    end

    fun write_x(int x): int
    begin
        write(x);
        return 0
    end

end

class B
begin

    A a;

    fun init()
    begin
        self.a := new A();
        return self
    end

    fun bar(int x): int
    begin
        return self.a.foo(x)
    end

end


class C
begin

    B b;

    fun init()
    begin
        self.b := new B();
        return self
    end

    fun faz(int x): int
    begin
        return self.b.bar(x)
    end

    fun get_b(): B
    begin
        return self.b
    end

    fun ord(int x, int y, int z): int
    begin
        write(x);
        write(y);
        write(z);
        return 0
    end

end


C c;
c := new C();

c.faz(5);
write(c.faz(5));
write(c.b.bar(5));
write(c.get_b().bar(5));
write(c.get_b().a.foo(5));
write(new C().faz(5));

c.get_b().a.write_x(123);
c.get_b().a.write_x(123)
